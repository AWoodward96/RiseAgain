CutsceneManager="*res://RESOURCES/Managers/CutsceneManager.tscn"

[debug]

gdscript/warnings/shadowed_global_identifier=0
gdscript/warnings/integer_division=0

[dialogic]

directories/dch_directory={
"character_knight": "res://RESOURCES/Timelines/Characters/character_knight.dch"
}
directories/dtl_directory={
"FTUE_Map_0": "res://RESOURCES/Timelines/Tutorial/FTUE_Map_0.dtl",
"test_timeline": "res://RESOURCES/Timelines/test_timeline.dtl"
}
variables={
"Butter": 1
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
