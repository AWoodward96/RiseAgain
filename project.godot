; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[Fmod]

General/auto_initialize=true
General/channel_count=1024
General/is_live_update_enabled=true
General/is_memory_tracking_enabled=false
"Software Format/sample_rate"=48000
"Software Format/speaker_mode"=3
"Software Format/raw_speaker_count"=0
General/default_listener_count=1
General/banks_path="res://AUDIO/Desktop"
General/should_load_by_name=false
DSP/dsp_buffer_size=512
DSP/dsp_buffer_count=4
"3D Settings/doppler_scale"=1.0
"3D Settings/distance_factor"=32.0
"3D Settings/rolloff_scale"=1.0

[application]

config/name="RiseAgain_Proto01"
run/main_scene="res://SCENES/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="RiseAgain"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://RESOURCES/Managers/GameManager.tscn"
WorldMap="*res://RESOURCES/Universe/WorldMap/WorldMap.tscn"
PersistDataManager="*res://RESOURCES/Managers/PersistDataManager.tscn"
UIManager="*res://RESOURCES/Managers/UIManager.tscn"
FmodManager="*res://addons/fmod/FmodManager.gd"
AudioManager="*res://RESOURCES/Managers/AudioManager.tscn"
InputManager="*res://SCRIPTS/Managers/InputManager.gd"
